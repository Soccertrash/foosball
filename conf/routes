# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# landing page
GET     /                           controllers.Application.index

# Navbar
GET		/navbar.html				controllers.Application.navbar

# Partials
GET     /partials/player.html		controllers.Application.player
GET     /partials/home.html		controllers.Application.home


# Player
GET		/Player/websocket			controllers.player.PlayerController.websocket

# Javascript routing
GET    /assets/javascripts/routes      controllers.JavascriptRouter.javascriptRoutes()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
